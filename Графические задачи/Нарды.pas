X0:=X00+2*D ; Y0:=Y00-3*YY-3*D; C:=9;YY:=0; A:=25; A1:=A*2+D;
A2:=A*3+D*2; B:=40; B1:=B*2+D; B2:=B*3+D*2;E:=0;B3:=15;
KUB1(X0+3*B3+9,Y0,C19); KUB1(X0+B+D+3*B3+6,Y0-YY-E,C20);
KUB1(X0+B1+D+3*B3+3,Y0-2*YY-2*E,C21); X0:=X00+D;
